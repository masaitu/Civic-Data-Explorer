{
    "name":  "civic-data-explorer",
    "version":  "1.0.0",
    "main":  "index.js",
    "scripts":  {
                    "lint":  "run-s lint:html lint:css",
                    "test":  "npm run lint",
                    "lint:html":  "htmlhint \"public/**/*.html\"",
                    "lint:css":  "stylelint \"public/**/*.css\"",
                    "audit:a11y":  "start-server-and-test dev http://localhost:4173 lighthouse:accessibility",
                    "dev":  "serve public -l 4173",
                    "lighthouse:accessibility":  "lighthouse http://localhost:4173 --only-categories=accessibility --output html --output-path evidence/lighthouse-accessibility.html --chrome-path=node_modules/chromium/lib/chromium/chrome-win/chrome.exe --chrome-flags=\"--headless --no-sandbox\""
                },
    "keywords":  [

                 ],
    "author":  "Civic Data Explorer Team",
    "license":  "MIT",
    "description":  "Upload 1 scaffold for Civic Data Explorer",
    "devDependencies":  {
                            "chromium":  "^3.0.3",
                            "htmlhint":  "^1.7.1",
                            "lighthouse":  "^12.8.2",
                            "npm-run-all":  "^4.1.5",
                            "serve":  "^14.2.5",
                            "start-server-and-test":  "^2.1.2",
                            "stylelint":  "^16.25.0",
                            "stylelint-config-standard":  "^39.0.1"
                        }
}
